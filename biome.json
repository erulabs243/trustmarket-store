{
	"$schema": "https://biomejs.dev/schemas/1.3.1/schema.json",
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"semicolons": "always",
			"arrowParentheses": "always",
			"trailingComma": "all"
		}
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
